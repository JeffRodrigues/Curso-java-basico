package jefferson.estudos.cursojava.aula11;

public class VariaveisChar {

	public static void main(String[] args) {
		
		//Declaração de Variáveis		
		//char o = 'o';		
		//char i = 'i';
		
		char o = 111;		
		char i = 105;
		char interrogacao = U+003F; // valor '?'
		
		System.out.println(""+o+i+ interrogacao);
  
		
	}

}
